--COFFEEMENU

DROP TABLE COFFEEMENU
CASCADE CONSTRAINTS;

DROP SEQUENCE SEQ_COFFEEMENU;

CREATE TABLE COFFEEMENU(
	MENUNUM NUMBER(8) PRIMARY KEY,
	C_NAME VARCHAR2(500) NOT NULL,
	C_SIZE VARCHAR2(200) NOT NULL,
	PRICE NUMBER(10) NOT NULL
)

CREATE SEQUENCE SEQ_COFFEEMENU
START WITH 1
INCREMENT BY 1;

SELECT * FROM COFFEEMENU;
